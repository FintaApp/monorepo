{
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "types/**/*.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"]
    },
    "strict": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}